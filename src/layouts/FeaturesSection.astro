---
import { Image } from "astro:assets";
import CustomImage from "../assets/Customize-Image.png";
---

<section class="bg-[#0056B3] rounded-t-xl">

    <!-- Background Grid -->
    <div class="bg-[linear-gradient(to_right,#f5f5f550_1px,transparent_1px),linear-gradient(to_bottom,#f5f5f550_1px,transparent_1px)] bg-[size:80px_100px] [mask-image:radial-gradient(ellipse_80%_90%_at_50%_50%,#000_90%,transparent_100%)]">
        
        <!-- Container Start -->
        <div class="mx-auto py-16 px-4 max-w-7xl">

            <div class="space-y-2 max-w-3xl mb-8">
                <span class="text-sm text-neutral-100 font-medium">Packed with Powerful Features</span>
                <h2 class="text-4xl text-neutral-50 font-bold">Everything You Need to Build Stunning, High-Performance Websites</h2>
            </div>
            
            <!-- Grids Start -->
            <div class="relative z-10 grid gap-3 grid-cols-6">

                <!-- Grid 1 Start -->
                <div class="group relative flex col-span-full lg:col-span-3 overflow-hidden p-8 rounded-xl bg-white/40 border border-neutral-200 backdrop-blur-sm">
                    
                    <!-- SVG -->
