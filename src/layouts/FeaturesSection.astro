                        <Image src={CustomImage} alt="Background image of code"></Image>
                    </div>
                    
                    <!-- Header -->
                    <div class="size-fit m-auto relative">
                        <div class="relative h-24 w-56 flex items-center z-10">
                            <span class="w-fit block mx-auto text-5xl font-bold text-transparent bg-clip-text bg-gradient-to-br from-white to-blue-300">100%</span>
                        </div>
